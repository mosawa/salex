<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="375.0" prefWidth="500.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="salex.controller.RootAreaViewController">
  <children>
    <TableView layoutX="14.0" layoutY="14.0" prefHeight="251.0" prefWidth="472.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="173.0" text="Code" fx:id="codeTableColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="297.0" text="Name" fx:id="nameTableColumn" />
      </columns>
    </TableView>
    <HBox id="HBox" alignment="CENTER" layoutX="14.0" layoutY="277.0" spacing="5.0">
      <children>
        <Label prefWidth="35.0" text="Code:" />
        <TextField fx:id="codeTextField" onAction="#gotoName" prefWidth="201.0" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER" layoutX="14.0" layoutY="303.0" spacing="5.0">
      <children>
        <Label text="Name:" />
        <TextField fx:id="nameTextField" onAction="#gotoUpdate" prefWidth="200.0" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER" layoutX="53.0" layoutY="334.0" spacing="20.0">
      <children>
        <Button fx:id="updateButton" mnemonicParsing="false" onAction="#update" text="Update" />
        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#delete" text="Delete" />
        <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clear" text="Clear" />
      </children>
    </HBox>
    <HBox layoutX="150.0" layoutY="138.0" prefHeight="100.0" prefWidth="200.0" />
  </children>
  <stylesheets>
    <URL value="@../style/Style.css" />
  </stylesheets>
</AnchorPane>
